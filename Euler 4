def palindrome_check(num):
  return str(num) == str(num)[::-1]

for i in range(100, 1000):
  number = i * i +1
  if palindrome_check(number) == True:
    print(str(i) + " and " + str(i+1))

def compute():
	ans = max(i * j
		for i in range(100, 1000)
		for j in range(100, 1000)
		if str(i * j) == str(i * j)[ : : -1])
	return str(ans)


if __name__ == "__main__":
	print(compute())
